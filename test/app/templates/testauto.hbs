<h1 class="u-ml-5 u-mt-5">Tokens</h1>
<table class="u-ml-5 u-mr-5 u-mt-5 u-mb-5">
  <tbody>
    {{#each model as |token|}}
      <tr>
        <td class="inner-token">
          <p class="u-floatLeft u-mr-2 u-mb-0">{{token.name}}</p>
          {{#each token.scopes as |scope|}}
            {{#tool-tip title="Token Scope"}}
              <p class="Badge Badge--uppercase Badge--grey u-floatLeft u-mr-2 u-mb-0">{{scope}}</p>
            {{/tool-tip}}
          {{/each}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
