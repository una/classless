{{#if showSketchyForm}}
{{sketchy-form
  data=componentData
  loading=loading
  model=model
  updateFirstName="updateFirstName"
  updateLastName="updateLastName"
  updatePhoneNumber="updatePhoneNumber"
  updateLocation="updateLocation"
  updateTypeOfTraffic="updateTypeOfTraffic"
  updateOtherAccounts="updateOtherAccounts"
  updateWhyNewAccount="updateWhyNewAccount"
  unauthenticate="unauthenticate"
  authenticate="authenticate"
  onSubmit="submitSketchyForm"
  deactivate="deactivate"
  onModalHide="onModalHide"
  submitButtonText="Submit Verification Request"
}}
{{else}}
  {{partial "partials/team-locked"}}
{{/if}}
